# Canva OAuth Configuration
# Get these values from your Canva app at https://www.canva.com/developers/apps
CANVA_CLIENT_ID=your_canva_client_id_here
CANVA_CLIENT_SECRET=your_canva_client_secret_here

# OAuth Redirect URI - should match what you set in your Canva app
CANVA_REDIRECT_URI=http://localhost:3000/auth/callback

# Session Secret - use a random string for production
SESSION_SECRET=your_random_session_secret_here

# Application Port (optional, defaults to 3000)
PORT=3000
